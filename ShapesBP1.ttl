:UmsatzsteuernummerKaeufer
    a sh:NodeShape ;
    sh:targetClass org:FormalOrganization ;
    sh:message "Das Segemnt RFF+VA fehlt für das Segemnet NAD+BY, d.h. Angbae des Umsatzsteuernummer des Kaufers; RFF; SG 3; Kopfteil;fehlt";
    sh:or (
        [ 
            sh:not 
            [
		        a sh:PropertyShape ;
		        sh:path edifact-o:hasFunctionInInvoice ;
		        sh:hasValue "Kaeufer" ;
            ] 
        ]
        [
            sh:path p2p-o-org:VATIdentifier ;
            sh:minCount 1;
            sh:maxCount 1;
            sh:maxLength 14 ;
        ]
    ) 
.

